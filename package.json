  "homepage": "https://github.com/aviadhahami/vue-chaos",
  "author": "Aviad Hahami <aviadhahami@gmail.com>",
  "main": "dist/vue-chaos.common.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "keywords": [
    "front-end",
    "web",
    "vue",
    "vuejs",
    "vue-js",
    "chaos-vue",
    "chaos",
    "chaos-engineering-vue"
  ],
  "bugs": {
    "url": "https://github.com/aviadhahami/vue-chaos/issues"
  },
  "scripts": {
    "demo": "vue-cli-service serve --open ./demo/index.js",
